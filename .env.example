# Database (PostgreSQL)
# Format: postgresql://user:password@host:port/dbname
DATABASE_URL=postgresql://aas:aas_demo@localhost:5432/aas

# Salesforce Integration
SF_USERNAME=your_salesforce_username
SF_PASSWORD=your_salesforce_password
SF_SECURITY_TOKEN=your_security_token
SF_DOMAIN=login  # or test, or specific domain

# Slack Integration
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
SLACK_CHANNEL=initial-channel-id

# Tableau Connected Apps (Embedding)
TABLEAU_SERVER_URL=https://10ax.online.tableau.com
TABLEAU_SITE_ID=agenticanalyticsstudio
TABLEAU_CONNECTED_APP_CLIENT_ID=your_client_id
TABLEAU_CONNECTED_APP_SECRET_ID=your_secret_id
TABLEAU_CONNECTED_APP_SECRET_VALUE=your_secret_value
TABLEAU_CONNECTED_APP_USERNAME=your_tableau_username

# Tableau REST API (Backend Data Access - Optional)
TABLEAU_TOKEN_NAME=your_token_name
TABLEAU_TOKEN_SECRET=your_token_secret

# Tableau Viz URLs (Specific Views)
# All URLs must contain "/views/"
TABLEAU_VIZ_URL_CLOUD=https://10ax.online.tableau.com/t/agenticanalyticsstudio/views/Superstore/Overview
TABLEAU_VIZ_URL_PIPELINE=https://10ax.online.tableau.com/t/agenticanalyticsstudio/views/Superstore/Overview
TABLEAU_VIZ_URL_CHURN=https://10ax.online.tableau.com/t/agenticanalyticsstudio/views/Superstore/Overview
TABLEAU_VIZ_URL_SPEND=https://10ax.online.tableau.com/t/agenticanalyticsstudio/views/Superstore/Overview

# AI / LLM Integration
LLM_PROVIDER=openai  # options: openai, ollama, none
OPENAI_API_KEY=sk-your-openai-api-key
OPENAI_MODEL=gpt-3.5-turbo

# Ollama Settings (Optional, for local LLMs)
OLLAMA_BASE_URL=http://localhost:11434/api
OLLAMA_MODEL=llama3

# Application Settings
LOG_LEVEL=INFO
PORT=8000
